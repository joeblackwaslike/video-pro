= form_for(@video) do |f|
  - if @video.errors.any?
    #error_explanation
      %h2
        = pluralize(@video.errors.count, "error")
        prohibited this video from being saved:
      %ul
        - @video.errors.full_messages.each do |msg|
          %li
            = msg
  .field
    = f.label :video_key
    %br/
    = f.text_field :video_key
  .actions
    = f.submit
